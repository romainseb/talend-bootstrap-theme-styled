{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "react/no-array-index-key": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "semi": ["error", "never"],
    "react/jsx-filename-extension": "off"
  }
}
